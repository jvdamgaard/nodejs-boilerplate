<!DOCTYPE html>
<html class="no-js">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">

  <title><%= title%></title>
  <meta name="description" content="">

  <meta name="robots" content="noindex, nofollow">

  <link rel="stylesheet" href="<%= cache['/css/style.css'] %>">
  <script src="<%= cache['/js/head.js'] %>"></script>
</head>
<body>
  <% if (errors.less.length > 0) { %>
  <div style="width:100%;padding:50px;background:rgba(255,0,0,0.9);color:white;font-size:16px;position:fixed;z-index:1000">
    <% errors.less.forEach(function(err,i) { %>
    <div style="font-size:28px;font-weight:bold;">LESS parse error</div>
    <div style="font-weight:bold;margin-top:30px;"><%- JSON.stringify(err.message).replace(/['"]/g,'')%></div>
    <div><%- JSON.stringify(err.filename).replace(/['"]/g,'')%> <%- JSON.stringify(err.line)%>:<%- JSON.stringify(err.column)%></div>
    <% }); %>
  </div>
  <% } %>

  <%-body -%>

  <script>
  var scripts = [];

  <% // Load jQuery as the first javascript element
  if (production) {
    %>scripts.push('//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js');<%
  } else {
    %>scripts.push('//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.js');<%
  }
  %>

  scripts.push('<%= cache["/js/global.js"] %>');

  <% // Load development javascript when not in production
  if (!production) {
    %>scripts.push('<%= cache["/js/development.js"] %>');<%
  }
  %>
  scripts = scripts.concat(ModuleLoader.modules);

  Modernizr.load(scripts);

  </script>
</body>
</html>